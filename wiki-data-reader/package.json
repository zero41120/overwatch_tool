{
  "name": "wiki-data-reader",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json && npm run update-items",
    "typecheck": "tsgo -p tsconfig.json --noEmit",
    "clean": "rm -rf dist",
    "postinstall": "npm run build",
    "refresh-snapshots": "tsx scripts/refreshSnapshots.ts",
    "update-items": "tsx scripts/updateItems.ts",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^1.6.1"
  }
}
